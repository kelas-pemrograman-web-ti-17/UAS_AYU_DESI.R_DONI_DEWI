aside.main-sidebar
    // sidebar: style can be found in sidebar.less
    section.sidebar
        // Sidebar user panel
        .user-panel
            .pull-left.image
                img.img-circle(src='/adminlte/dist/img/sepatu.jpg' alt='User Image')
            .pull-left.info
                p #{session_store.username}
                a(href='#')
                    i.fa.fa-circle.text-success
                    |  Online
        ul.sidebar-menu(data-widget='tree')
            li.header MAIN NAVIGATION
                li.treeview
                    a(href='/')
                        i.fa.fa-dashboard
                        span Dashboard
                if(session_store.admin == true)
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Sepatu
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputsepatu')
                                    i.fa.fa-circle-o
                                    |  Input Data Sepatu
                            li
                                a(href='/datasepatu')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Sepatu
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Pelanggan
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputpelanggan')
                                    i.fa.fa-circle-o
                                    |  Input Data Pelanggan
                            li
                                a(href='/datapelanggan')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Pelanggan
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Penjualan
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputpenjualan')
                                    i.fa.fa-circle-o
                                    |  Input Data Penjualan
                            li
                                a(href='/datapenjualan')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Penjualan
                else
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Sepatu
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/datasepatumember')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Sepatu
                            // /.box-body


    // /.sidebar